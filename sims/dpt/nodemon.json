{
	"restartable": "rs",
	"ignore": [
		".git",
		"node_modules/**/node_modules"
	],
	"verbose": true,
	"execMap": {
		"js": "node --harmony"
	},
	"events": {
		"restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
	},
	"watch": [
		"*.*",
		"../../src/server/config/spellsConfig.js",
		"../../src/server/combat/combat.js"
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js json"
}
